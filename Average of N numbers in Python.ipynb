     "text": [
      "How many numbers ? 4\n",
      "Enter any numbers 12\n",
      "Enter any numbers 54\n",
      "Enter any numbers 34\n",
      "Enter any numbers 23\n",
      "The average is  30.75\n"
     ]
    }
   ],
   "source": [
    "num = int(input(\"How many numbers ? \"))\n",
    "total_sum = 0\n",
    "for i in range(num):\n",
    "    number = float(input(\"Enter any numbers \"))\n",
    "    total_sum += number\n",
    "avg = total_sum / num\n",
    "print(\"The average is \", avg)\n"
   ]
